{
    "name": "roblopez/booking",
    "description": "booking",
    "type": "project",
    "license": "proprietary",
    "repositories": [],
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/dbal": "^3.2",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "guzzlehttp/guzzle": "^7.8",
        "lambdish/phunctional": "^2.1",
        "ramsey/uuid": "^4.7",
        "symfony/console": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^6.4",
        "symfony/messenger": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "require-dev": {
        "behat/behat": "^3.13",
        "behat/mink-browserkit-driver": "2.1.0",
        "friends-of-behat/mink-extension": "2.6.1",
        "friends-of-behat/symfony-extension": "2.3.1",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.1",
        "symplify/easy-coding-standard": "^9.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "Booking\\App\\": "src/"
            "Booking\\": ["src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Booking\\Tests\\": ["tests/src"]
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.4"
        }
    }
}
